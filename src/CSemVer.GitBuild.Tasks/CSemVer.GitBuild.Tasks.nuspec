<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>CSemVer.GitBuild.Tasks</id>
    <authors>CSemVer.GitBuild.Tasks</authors>
    <owners>Ubiquity.NET</owners>
    <version>$PackageVersion$</version>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
    <licenseUrl>https://github.com/UbiquityDotNET/CSemVer.GitBuild/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/UbiquityDotNET/CSemVer.GitBuild</projectUrl>
    <description>Provides standardized Constrained Semantic version numbers for builds including Local builds, Pull Request/Buddy Builds, CI builds and, of course official release builds</description>
    <tags>Build, Versioning, SemVer, CSemVer, CI Build</tags>
    <repository type="Git" url="https://github.com/UbiquityDotNET/CSemVer.GitBuild" />
  </metadata>
  <files>
    <file src="$reporoot$BuildOutput\bin\CSemVer.GitBuild.Tasks\$configuration$\**\*.*" target="tasks"/>
    <file src="$reporoot$src\CSemVer.GitBuild.Tasks\build\CSemVer.GitBuild.Tasks.props" target="build\CSemVer.GitBuild.Tasks.props" />
    <file src="$reporoot$src\CSemVer.GitBuild.Tasks\build\CSemVer.GitBuild.Tasks.targets" target="build\CSemVer.GitBuild.Tasks.targets" />
    <file src="$reporoot$src\CSemVer.GitBuild.Tasks\buildMultiTargeting\CSemVer.GitBuild.Tasks.props" target="buildMultiTargeting\CSemVer.GitBuild.Tasks.props" />
    <file src="$reporoot$src\CSemVer.GitBuild.Tasks\buildMultiTargeting\CSemVer.GitBuild.Tasks.targets" target="buildMultiTargeting\CSemVer.GitBuild.Tasks.targets" />
  </files>
</package>
