<Project TreatAsLocalProperty="_TaskFolder">
    <PropertyGroup>
        <_TaskFolder Condition="'$(MSBuildRunTimeType)'=='Core'">netstandard1.3</_TaskFolder>
        <_TaskFolder Condition="'$(MSBuildRunTimeType)'!='Core'">net46</_TaskFolder>
        <CSemVerGitBuildTasksAssembly>$([MSBuild]::NormalizePath("$(MSBuildThisFileDirectory)", "..", "tasks", "$(_TaskFolder)", "CSemVer.GitBuild.dll"))</CSemVerGitBuildTasksAssembly>
        <IsCIBuild Condition="'$(IsCIBuild)'=='' AND $(CI)" >true</IsCIBuild>
        <IsCIBuild Condition="'$(IsCIBuild)'==''" >false</IsCIBuild>
    </PropertyGroup>
</Project>
